{
	"manifest_version": 2,
	"name": "Anonygram",
	"version": "1.0",
    "description": "Browse Instagram profiles without having to login.",

	"content_scripts": [
	{
        "matches": ["https://www.instagram.com/*"],
		"js": ["jquery-1.9.1.min.js", "content.js"]
		}
	],
    
    "browser_action":
    {
        "default_icon" : "icon16.png",
        "default_popup" : "popup.html"
        
    },
    
    "icons": 
    {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon48.png"
    },
    
    
    "permissions": [
        "activeTab",
        "tabs"
   ]
}
